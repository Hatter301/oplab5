#include<math.h>
#include <stdio.h>
int main()
{
   const int p=10;
   int f=0,t,i=0,a[p],h;
   double d;
   printf("Введите элементы массива\n");
   while (i!=p) {
       scanf("%d",&a[i]);
       i++;
   }
   d=p/1.247;
   h=round(d);
   while(h!=0) {
       for(i=0;i+h!=p;i++)
       if(a[i]>a[i+h]) {
           t=a[i];
           a[i]=a[i+h];
           a[i+h]=t;
       }
       d=d/1.247;
       h=round(d);
   }
   for (i=0;i<p;i++)
   printf("%d ",a[i]);
    return 0;
}
